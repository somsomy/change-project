<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.somsomy.mapper.AdoptMapper">
	<select id="getMaxNum" resultType="java.lang.Integer">
		select max(num) from adoptboard
	</select>
	
	<select id="getBoardList" resultType="com.somsomy.domain.AdoptBean">
		select * from adoptboard order by num desc limit #{startRow}, #{pageSize}
	</select>
</mapper>